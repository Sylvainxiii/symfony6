{{ form_start(form) }}

<div class = 'row'>
    <div class = 'col'>
    {{form_row(form.email)}}
    </div>
</div>

<div class = 'row'>
    <div class = 'col'>
    {{form_row(form.roles,{value:form.vars.data.roles[0]})}}
    </div>
</div>

<div class = 'row'>
    <div class = 'col'>
    {{form_row(form.nom)}}
    </div>
</div>

<div class = 'row'>
    <div class = 'col'>
    {{form_row(form.prenom)}}
    </div>
</div>

    <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
